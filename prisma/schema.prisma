
generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  password  String
  name      Name
  city      String?
  country   String?
  phone     String?
  street    String?
}

type Name{
  firstname String
  lastname String
}